{
	"openapi": "3.0.0",
	"info": {
		"version": "1.0.0",
		"title": "template",
		"license": {
			"name": "MIT"
		}
	},
	"servers": [
		{
			"url": "http://localhost:8080/auth/api"
		}
	],
	"paths": {
		"/test": {
			"get": {
				"summary": "Test API connection",
				"responses": {
					"200": {
						"description": "Test API connection",
						"headers": {
							"x-next": {
								"description": "A link to the next page of responses",
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "unexpected error",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/login": {
			"post": {
				"summary": "Log-in current user",
				"responses": {
					"200": {
						"description": "User login successful",
						"headers": {
							"x-next": {
								"description": "A link to the next page of responses",
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "User login failed",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/logout": {
			"get": {
				"summary": "Log-out current user",
				"responses": {
					"200": {
						"description": "User logout successful",
						"headers": {
							"x-next": {
								"description": "A link to the next page of responses",
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "User logout failed",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/verify": {
			"get": {
				"summary": "Verifying User",
				"responses": {
					"200": {
						"description": "User verification successful",
						"headers": {
							"x-next": {
								"description": "A link to the next page of responses",
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "User verification failed",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/register": {
			"post": {
				"summary": "Register a new User",
				"responses": {
					"200": {
						"description": "User registration successful",
						"headers": {
							"x-next": {
								"description": "A link to the next page of responses",
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "User registration failed",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		}
	}
}
